msgid "WP LinkedIn Multi-Users"
msgstr "WP LinkedIn Multi-Utilisateurs"

#. Plugin URI of the plugin/theme
msgid ""
"http://vedovini.net/plugins/?"
"utm_source=wordpress&utm_medium=plugin&utm_campaign=wp-linkedin-mu"
msgstr ""
